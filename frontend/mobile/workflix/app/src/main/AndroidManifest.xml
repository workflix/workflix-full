                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.perfil.perfil_terminos.PerfilTerminosActivity" />
        </activity>
        <activity
            android:name=".views.ui.perfil.eliminar_cuenta.EliminarCuentaActivity"
            android:exported="false"
            android:label="Eliminar Cuenta"
            android:parentActivityName=".views.ui.perfil.Perfil"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.perfil.Perfil" />
        </activity>
        <activity
            android:name=".views.ui.perfil.perfil_terminos.PerfilTerminosActivity"
            android:exported="false"
            android:label="Perfil"
            android:parentActivityName=".views.ui.auth.login.LoginActivity"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.auth.login.LoginActivity" />
        </activity>
        <activity
            android:name=".views.ui.auth.login.register.RegisterActivity"
            android:exported="false"
            android:label="Registro"
            android:parentActivityName=".views.ui.auth.login.LoginActivity"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.auth.login.LoginActivity" />
        </activity>
        <activity
            android:name=".views.ui.auth.login.LoginActivity"
            android:exported="false"
            android:label="Login"
            android:parentActivityName=".views.MainActivity"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.MainActivity" />
        </activity>
        <activity
            android:name=".views.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.ui.tarjetas.TarjetaAmpliadaActivity"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>