<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Workflix"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".views.ui.dashboard.DashboardAdminVistaActivity"
            android:exported="false"
            android:label="Administradores"
            android:parentActivityName=".views.ui.dashboard.DashboardAdminActivity"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.dashboard.DashboardAdminActivity" />
        </activity>
        <activity
            android:name=".views.ui.dashboard.DashboardServiciosActivity"
            android:exported="false"
            android:label="Servicios"
            android:parentActivityName=".views.ui.dashboard.DashboardAdminActivity"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.dashboard.DashboardAdminActivity" />
        </activity>
        <activity
            android:name=".views.ui.adapters.ServicioActivity"
            android:exported="false"
            android:label="Editar Servicios"
            android:parentActivityName=".views.ui.dashboard.DashboardServiciosActivity"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.dashboard.DashboardServiciosActivity" />
        </activity>
        <activity
            android:name=".views.ui.dashboard.DashboardUsuariosActivity"
            android:exported="false"
            android:label="Usuarios"
            android:parentActivityName=".views.ui.dashboard.DashboardAdminActivity"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.dashboard.DashboardAdminActivity" />
        </activity>
        <activity
            android:name=".views.ui.adapters.UsuarioActivity"
            android:exported="false"
            android:label="Editar Usuarios"
            android:parentActivityName=".views.ui.dashboard.DashboardUsuariosActivity"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.dashboard.DashboardUsuariosActivity" />
        </activity>
        <activity
            android:name=".views.ui.dashboard.DashboardActivity"
            android:exported="false" />

        <activity
            android:name=".views.ui.perfil.perfil_terminos.terminosCondiciones"
            android:exported="false"
            android:label="Terminos y Condiciones"
            android:parentActivityName=".views.ui.perfil.perfil_terminos.PerfilTerminosActivity"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.perfil.perfil_terminos.PerfilTerminosActivity" />
        </activity>
        <activity
            android:name=".views.ui.dashboard.DashboardAdminActivity"
            android:exported="false"
            android:label="Dashboard"
            android:parentActivityName=".views.MainActivity"
            android:theme="@style/Theme.Workflix2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.MainActivity" />
        </activity>
        <activity
            android:name=".views.ui.tarjetas.CatalogoActivity"
            android:exported="false"
            android:label="Catalogo de Profesionales"
            android:parentActivityName=".views.ui.tarjetas.CatalogoActivity"
            android:theme="@style/Theme.Workflix2">
