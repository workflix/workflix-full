                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.ui.auth.login.LoginActivity" />
        </activity>
        <activity
            android:name=".views.ui.perfil.Perfil"
            android:exported="false"
            android:label="Editar Perfil"
            android:parentActivityName=".views.ui.perfil.perfil_terminos.PerfilTerminosActivity"
