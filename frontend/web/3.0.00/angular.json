{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "servicio-libre-web": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/servicio-libre-web",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              
              "node_modules/bootstrap-icons/font/bootstrap-icons.min.css",
              "src/assets/css/vendor.bundle.base.css",              
              "src/assets/css/style.css",
              "src/styles.css"
            ],
            "scripts": [
              
              
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/js/vendor.bundle.base.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/jquery.cookie.js",
              "src/assets/js/off-canvas.js",
              "src/assets/js/hoverable-collapse.js",
              "src/assets/js/misc.js",
              "src/assets/js/dashboard.js",
              "src/assets/js/todolist.js"


            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "servicio-libre-web:build:production"
            },
            "development": {
              "buildTarget": "servicio-libre-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "servicio-libre-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.min.css",
              
              "src/assets/css/vendor.bundle.base.css",
              "src/assets/css/style.css",
              "src/styles.css"
            ],
            "scripts": [
              
              
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/js/vendor.bundle.base.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/jquery.cookie.js",
              "src/assets/js/off-canvas.js",
              "src/assets/js/hoverable-collapse.js",
              "src/assets/js/misc.js",
              "src/assets/js/dashboard.js",
              "src/assets/js/todolist.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "399dc623-a89d-4073-9c07-d2eba050c1ad"
  }
}
